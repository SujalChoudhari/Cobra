import "str"; // Assuming the loader will map this correctly

var s1 = new String("   hello world   ");

// Chainable, immutable operations
var s2 = s1.trim().toUpperCase().replace("WORLD", "COBRA");

print("Original: '" + s1.toString() + "'");
print("Modified: '" + s2.toString() + "'");
print("Length of modified: " + s2.length());
print("Does it start with 'HELLO'? " + s2.startsWith("HELLO"));